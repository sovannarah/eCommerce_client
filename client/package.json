{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.0",
    "@material-ui/icons": "^4.2.0",
    "axios": "^0.19.0",
    "boostrap": "^2.0.0",
    "fetch": "^1.1.0",
    "material-table": "^1.40.1",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-animated-slider": "^1.1.4",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.0.4",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-stripe-elements": "latest",
    "react-transition-group": "^4.2.1",
    "shallowequal": "^1.1.0"
  },
  "scripts": {
    "start": " react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "rules": {
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "no-console": [
        2,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "arrow-parens": [
        2,
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "block-scoped-var": 2,
      "camelcase": 2,
      "computed-property-spacing": [
        2,
        "never"
      ],
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "eqeqeq": [
        2,
        "smart"
      ],
      "keyword-spacing": 2,
      "max-depth": [
        1,
        3
      ],
      "max-len": [
        1,
        180
      ],
      "max-statements": [
        1,
        40
      ],
      "max-params": [
        1,
        4
      ],
      "new-cap": 1,
      "max-nested-callbacks": [
        2,
        4
      ],
      "no-extend-native": 2,
      "no-else-return": 1,
      "no-lonely-if": 1,
      "no-path-concat": 2,
      "no-unused-vars": [
        2,
        "all"
      ],
      "no-undef": 1,
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-floating-decimal": 1,
      "no-mixed-requires": [
        1,
        true
      ],
      "no-useless-escape": 2,
      "no-sync": 0,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "space-unary-ops": 2,
      "semi": [
        2,
        "always"
      ],
      "no-unneeded-ternary": [
        2,
        {
          "defaultAssignment": false
        }
      ]
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-sass-guidelines"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
